<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
      <script src="https://cdn.tailwindcss.com"></script>
</head>
<body>
    <div class="w-full h-full p-10 bg-zinc-900 text-white">

        <div class="nav mb-10 p-4 w-full">
            <a class="text-blue-600" href="./read">Read Users</a>
        </div>

        <h1 class="text-3xl text-white tracking-tighter mb-3">Create User</h1>
        <form class="" action="/create" method="post">
            <input class="px-2 py-3 bg-transparent border-2 border-zinc-800 rounded-lg outline-none" type="text" name="name" placeholder="name">
            <input class="px-2 py-3 bg-transparent border-2 border-zinc-800 rounded-lg outline-none" type="text" name="email" placeholder="email">
            <input class="px-2 py-3 bg-transparent border-2 border-zinc-800 rounded-lg outline-none" type="text" name="image" placeholder="image-url">
             <input class="px-2 py-3 bg-transparent border-2 border-zinc-800 rounded-lg outline-none" type="image" name="image-up" placeholder="Upload-Image">
            <input class="px-3 py-3 bg-blue-600 border-2 border-zinc-800 rounded-lg outline-none" type="submit" placeholder="Submit">
            
          
        </form>

        
    </div>
    
